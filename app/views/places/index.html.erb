<h1>places#index</h1>
<p>Find me in app/views/places/index.html.erb</p>

<% @places.each do |place| %>
<p> <%=image_tag place.image[0].thumb.url%> <%= link_to "#{place.name}", place_path(place)%> </p>
<% end %>
<p><%=link_to "Home", user_path(current_user)%> </p>

<%= form_tag places_path, :method => 'get' do %>
 <p>
   <%= text_field_tag :query, params[:query] %>
   <%= submit_tag "Search", :name => nil %>
 </p>

<%end %>
<div class="other-links">
  <p><%=link_to "Home", user_path(current_user)%>
      <%=link_to "Index", places_path%></p>
</div>
